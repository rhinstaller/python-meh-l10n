#: ../meh/handler.py:195
msgid ""
"Use 'continue' command to quit the debugger and get back to the main menu"
msgstr ""
"Použite príkaz 'continue' na ukončenie ladiaceho nástroja a návrat do "
"hlavného menu"

#: ../meh/handler.py:211
msgid "Exit the shell to get back to the main menu"
msgstr "Ukončíte príkazový riadok pre návrat do hlavného menu"

#. short (one-char) answer meaning "yes"
#: ../meh/ui/text.py:75
msgid "y"
msgstr "a"

#. short (one-char) answer meaning "no"
#: ../meh/ui/text.py:78
msgid "n"
msgstr "m"

#: ../meh/ui/text.py:145
msgid "Save exception"
msgstr "Uložiť výnimku"

#: ../meh/ui/text.py:158 tmp/exception-dialog.glade.h:1
msgid "An unknown error has occurred"
msgstr "Vyskytla sa neznáma chyba"

#: ../meh/ui/text.py:161
msgid "Report Bug"
msgstr "Nahlásiť chybu"

#: ../meh/ui/text.py:162
msgid "Run shell"
msgstr "Spustiť príkazový riadok"

#: ../meh/ui/text.py:163
msgid "Quit"
msgstr "Ukončiť"

#: ../meh/ui/text.py:168
msgid "Debug"
msgstr "Ladenie"

#: ../meh/ui/text.py:173
msgid "What do you want to do now?"
msgstr "Ako chcete pokračovať?"

#: ../meh/ui/text.py:181
msgid "Please make your choice from above: "
msgstr "Prosím vyberte z vyššie uvedených možností: "

#: ../meh/ui/text.py:199
msgid "Hit ENTER to continue"
msgstr "Stlačte ENTER na pokračovanie"

#: ../meh/ui/text.py:213
#, python-format
msgid "Are you sure you want to exit? [%(yes)s/%(no)s]"
msgstr "Ste si istý, že chcete ukončiť? [%(yes)s/%(no)s]"

#: ../meh/ui/gui.py:176
msgid "_Exit"
msgstr "_Ukončiť"

#: tmp/exception-dialog.glade.h:2
msgid ""
"This program has encountered an unknown error. You may\n"
"report the bug below or quit the program."
msgstr ""
"Program narazil na neznámu chybu. Nižšie môžete \n"
"nahlásiť chybu alebo ukončiť program."

#: tmp/exception-dialog.glade.h:4
msgid "_Report Bug"
msgstr "_Nahlásiť chybu"

#: tmp/exception-dialog.glade.h:5
msgid "_Quit"
msgstr "_Ukončiť"

#: tmp/exception-dialog.glade.h:6
msgid "The output below may help determine the cause of the error:"
msgstr "Nižšie uvedený výstup môže pomôcť určiť príčinu chyby:"

#: tmp/exception-dialog.glade.h:7
msgid "'Debug' may take you to tty1."
msgstr "'Ladenie' vás môže prepnúť do tty1."

#: tmp/exception-dialog.glade.h:8
msgid "_Debug"
msgstr "_Ladenie"

#: tmp/exception-dialog.glade.h:9
msgid "More _info..."
msgstr "Viac _informácií..."

#: tmp/exception-dialog.glade.h:10
msgid "No additional info available."
msgstr "Nie sú dostupné žiadne ďalšie informácie."
